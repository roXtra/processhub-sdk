{
  "name": "processhub-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/roXtra/processhub-sdk.git"
  },
  "version": "8.10.0-5",
  "author": {
    "name": "ProcessHub",
    "email": "info@processhub.com",
    "url": "https://github.com/roXtra/processhub-sdk"
  },
  "bugs": {
    "url": "https://github.com/roXtra/processhub-sdk/issues"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "branches": 5,
    "lines": 10,
    "functions": 5,
    "statements": 10,
    "exclude": []
  },
  "main": "lib/index.js",
  "types": "lib/index",
  "deprecated": false,
  "description": "ProcessHub SDK for JavaScript/TypeScript",
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "bpmn-moddle": "^4.0.0",
    "chai": "^4.2.0",
    "fetch-everywhere": "^1.0.5",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "hapi": "^17.2.1",
    "html5-desktop-notifications": "^1.0.0",
    "immutability-helper": "^2.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nes": "^6.5.2",
    "react": "^16.9.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build": "tsc && babel tsc_out --out-dir lib && npm run copytypings",
    "test": "npm run lint && npm run testcoverage",
    "lint": "eslint --ext .ts,.tsx ./src",
    "testall": "cross-env HOTRELOAD=no ts-mocha -p tsconfig.json ./src/**/*.test.ts --exit --timeout 10000 --recursive --require regenerator-runtime",
    "testcoverage": "nyc --reporter=lcov --reporter=text-summary npm run testall",
    "copytypings": "copyfiles -u 1 \"src/**/*.d.ts\" lib && copyfiles -u 1 \"src/*.d.ts\" lib && copyfiles -u 1 \"tsc_out/**/*.d.ts\" lib && copyfiles -u 1 \"tsc_out/*.d.ts\" lib && copyfiles -u 1 src/process/types/index.d.ts lib",
    "jscpd": "jscpd ./src --ignore \"*/**/*.test.ts\"",
    "release-switch-to-next-rc": "npm version preminor",
    "release-bump-preview-version": "npm version prerelease",
    "release-make-next-rc-final": "npm version minor"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/hapi": "^17.0.12",
    "@types/immutability-helper": "^2.0.15",
    "@types/lodash": "^4.14.65",
    "@types/mocha": "^5.2.7",
    "@types/nes": "^7.0.5",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.3",
    "@types/react-router-redux": "^5.0.18",
    "@types/request": "0.0.44",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "chai-exclude": "^2.0.2",
    "copyfiles": "^2.1.1",
    "cross-env": "^6.0.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.15.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "regenerator-runtime": "^0.13.3",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "lib/",
    "src/"
  ],
  "homepage": "https://github.com/processhub/processhub-sdk",
  "keywords": [
    "processhub",
    "bpm",
    "process",
    "bpmn",
    "bpmn 2"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/processhub/processhub-sdk/master/LICENSE"
    }
  ],
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "os-browserify",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
