{
  "name": "processhub-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/roXtra/processhub-sdk.git"
  },
  "version": "9.32.0-3",
  "author": {
    "name": "ProcessHub",
    "email": "info@processhub.com",
    "url": "https://github.com/roXtra/processhub-sdk"
  },
  "engines": {
    "node": "^v14.18.3",
    "npm": "^6.14.15"
  },
  "bugs": {
    "url": "https://github.com/roXtra/processhub-sdk/issues"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "branches": 5,
    "lines": 10,
    "functions": 5,
    "statements": 10,
    "exclude": []
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "deprecated": false,
  "description": "ProcessHub SDK for JavaScript/TypeScript",
  "dependencies": {
    "@progress/kendo-data-query": "^1.5.5",
    "@progress/kendo-react-charts": "4.9.0",
    "@progress/kendo-react-data-tools": "4.8.0",
    "@progress/kendo-react-dropdowns": "4.9.0",
    "@progress/kendo-react-grid": "4.8.0",
    "@progress/kendo-react-treeview": "4.9.0",
    "base-64": "^1.0.0",
    "bpmn-moddle": "^7.1.2",
    "cross-fetch": "^3.1.4",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "murmurhash": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sqlstring": "^2.3.2",
    "utf8": "^3.0.0",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "build": "rimraf ./lib && tsc && npm run copytypings",
    "test": "npm run build && npm run lint && npm run testcoverage",
    "lint": "eslint --ext .ts,.tsx ./src",
    "testall": "ts-mocha ./src/**/*.test.ts --exit --timeout 10000 --recursive",
    "testcoverage": "nyc --reporter=lcov --reporter=text-summary npm run testall",
    "copytypings": "copyfiles -u 1 \"src/**/*.d.ts\" lib && copyfiles -u 1 \"src/*.d.ts\" lib && copyfiles -u 1 src/process/types/index.d.ts lib",
    "jscpd": "jscpd ./src --ignore \"*/**/*.test.ts\"",
    "release-bump-preview-version": "npm run check:format && npm run lint && npm version prerelease",
    "check:format": "prettier --config .prettierrc -c ./src",
    "format": "prettier --config .prettierrc -w ./src"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/base-64": "^1.0.0",
    "@types/chai": "^4.3.0",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.5",
    "@types/react": "^17.0.38",
    "@types/sqlstring": "^2.3.0",
    "@types/utf8": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "chai": "^4.3.4",
    "chai-exclude": "^2.1.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.5.4"
  },
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "lib/",
    "src/"
  ],
  "homepage": "https://github.com/processhub/processhub-sdk",
  "keywords": [
    "processhub",
    "bpm",
    "process",
    "bpmn",
    "bpmn 2"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/processhub/processhub-sdk/master/LICENSE"
    }
  ],
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "os-browserify",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
